{
  "name": "@newhighsco/next-plugin-svgr",
  "description": "Next.js plugin for transforming SVGs into React components using SVGR",
  "version": "3.0.157",
  "author": "New High Score",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/next-plugin-svgr.git"
  },
  "homepage": "https://github.com/newhighsco/next-plugin-svgr#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/next-plugin-svgr/issues"
  },
  "main": "src/index.js",
  "files": [
    "src/webpack.js"
  ],
  "scripts": {
    "prepare": "husky",
    "pretest": "yarn lint",
    "test": "jest --runInBand",
    "test:list": "jest --listTests",
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "format": "concurrently yarn:format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@svgr/webpack": "8.1.0",
    "file-loader": "6.2.0",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/preset-env": "7.28.0",
    "@commitlint/cli": "19.8.1",
    "@newhighsco/commitlint-config": "1.1.46",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.1.40",
    "@newhighsco/prettier-config": "2.1.32",
    "@newhighsco/release-config": "1.4.6",
    "babel-jest": "30.0.4",
    "concurrently": "9.2.0",
    "eslint": "8.57.1",
    "husky": "9.1.7",
    "jest": "30.0.4",
    "jest-junit": "16.0.0",
    "prettier": "3.6.2",
    "semantic-release": "24.2.7"
  },
  "peerDependencies": {
    "next": "15.4.2"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "local>newhighsco/.github:renovate-config"
    ]
  }
}
