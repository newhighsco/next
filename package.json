{
  "name": "@newhighsco/next-plugin-svgr",
  "description": "Next.js plugin for transforming SVGs into React components using SVGR",
  "version": "3.0.131",
  "author": "New High Score",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/next-plugin-svgr.git"
  },
  "homepage": "https://github.com/newhighsco/next-plugin-svgr#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/next-plugin-svgr/issues"
  },
  "main": "src/index.js",
  "files": [
    "src/webpack.js"
  ],
  "scripts": {
    "prepare": "husky",
    "pretest": "yarn lint",
    "test": "concurrently yarn:test:*",
    "test:js": "jest --coverage",
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "format": "concurrently yarn:format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@svgr/webpack": "8.1.0",
    "file-loader": "6.2.0",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@commitlint/cli": "19.4.0",
    "@newhighsco/commitlint-config": "1.1.41",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.0.207",
    "@newhighsco/prettier-config": "2.1.26",
    "@newhighsco/release-config": "1.3.79",
    "babel-jest": "29.7.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "semantic-release": "24.0.0"
  },
  "peerDependencies": {
    "next": "14.2.5"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "local>newhighsco/.github:renovate-config"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:nextjs"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
