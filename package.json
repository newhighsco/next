{
  "name": "@newhighsco/next-plugin-svgr",
  "description": "Next.js plugin for transforming SVGs into React components using SVGR",
  "version": "3.0.48",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/next-plugin-svgr.git"
  },
  "homepage": "https://github.com/newhighsco/next-plugin-svgr#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/next-plugin-svgr/issues"
  },
  "main": "src/index.js",
  "files": [
    "src/webpack.js"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@svgr/webpack": "6.2.1",
    "file-loader": "6.2.0",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@commitlint/cli": "16.2.3",
    "@newhighsco/commitlint-config": "1.1.0",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "3.2.5",
    "@newhighsco/prettier-config": "2.1.1",
    "@newhighsco/release-config": "1.3.4",
    "babel-jest": "27.5.1",
    "eslint": "8.13.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2"
  },
  "peerDependencies": {
    "next": "12.1.4"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:nextjs"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
